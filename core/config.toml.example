[app]
debug = false
cache_dir = "./cache/"

[database]
host = "localhost"
port = 3306
user = "manyacg"
password = ""
database = "manyacg"


[log]
level = "TRACE"
file_path = "./logs/core.log"
backup_num = 5

[middleware]
[middleware.messenger]
type = "rabbitmq"

[middleware.messenger.azure]
bus_connection_string = ""
sub_topic = "manyacg-collector"
subscription = "core"
pub_topic = "manyacg-core"

[middleware.messenger.rabbitmq]
host = "192.168.1.175"
port = 5672
user = "admin"
password = ""
vhost = "manyacg"
sub_exchange = "manyacg-collector"
sub_queue = "manyacg-core-sub-queue"
pub_exchange = "manyacg-core"


[middleware.redis]
addr = "localhost:6379"
password = ""
db = 0
cache_ttl = 30


[processor]
enable_ext = false
max_concurrent = 10

[processor.donwload]

[processor.save]
type = "webdav"

[processor.save.local]
path = "./"

[processor.save.webdav]
url = ""
username = ""
password = ""
path = "./"
cache_ttl = 30


[grpc]
address = "0.0.0.0:39110"
cert = "./keys/server.crt"
key = "./keys/server.key"
ca = "./keys/ca.crt"

[api]
enable = true
address = "0.0.0.0:39120"
